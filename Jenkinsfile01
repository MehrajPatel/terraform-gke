pipeline {
    agent any
    tool {
      maven 3.8.6
     }

    stages {
        stage('Git Clone') {
            steps {
                git credentialsId: 'GITHUB_CREDENTIALS', url: 'https://github.com/quadrimuzammil/terraform-gke.git'
            }
        }
     stage('mvn build') {
            steps {
                sh 'mvn clean package'
            }
        }   
  stage('Build Docker Image') {
            steps {
                docker build -t quadrimuzammil/javaapp:${BuildNumber} .
            }
        }
  
  
  
  }
    }
